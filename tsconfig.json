{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "types": ["./src/env.d.ts"],
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "paths": {
      "@components/*": ["src/components/*"],
      "@layouts/*": ["src/layouts/*"],
      "@lib/*": ["src/lib/*"],
      "@pages/*": ["src/pages/*"],
      "@src/*": ["src/*"],
      "@root/*": ["./*"]
    }
  }
}
